@{
    ViewBag.Title = "DATA EXTRACTION";
    ViewBag.Redirect = "reportSystem";
    Layout = "~/Views/Shared/_Modal.cshtml";
}

<div class="container-fluid">
    <div class="col-12">
        <div class="card mb-4">
            <div class="card-header"><h3>DATA EXTRACTION</h3></div>
            <div class="card-body">
                <table id="extraction-table" class="table-hover" style="width:100%; font-size:10px;">
                    <thead>
                        <tr>
                            <th>M3 #</th>
                            <th>TESTER ID</th>
                            <th>HANDLER ID</th>
                            <th>PACKAGE</th>
                            <th>FAMILY</th>
                            <th>PROCESS</th>
                            <th>DATE REQUEST</th>
                            <th>EXPECTED DATE OF SETUP</th>
                            <th>SHIFT</th>
                            <th>REQUESTED BY</th>
                            <th>STATUS</th>
                        </tr>
                    </thead>
                    <tbody id="data-extraction"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
